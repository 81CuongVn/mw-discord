		"PageContentInsertComplete": [
			"MediawikiDiscordHooks::onPageContentInsertComplete"	
		],
		"TitleMoveComplete": [
			"MediawikiDiscordHooks::onTitleMoveComplete"	
		],
		"ArticleDeleteComplete": [
			"MediawikiDiscordHooks::onArticleDeleteComplete"	
		],
		"ArticleUndelete": [
			"MediawikiDiscordHooks::onArticleUndelete"	
		],
		"ArticleProtectComplete": [
			"MediawikiDiscordHooks::onArticleProtectComplete"	
		]
	}
