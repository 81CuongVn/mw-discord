{
	"name": "mediawiki-discord",
	"author": "Szymon Zak",
	"url": "https://github.com/Szmyk/mediawiki-discord",
	"license-name": "MIT",
	"manifest_version": 1,
	"config": {
		"DiscordWebhookUrl": ""
	},
	"AutoloadClasses": {
		"MediawikiDiscordHooks": "MediawikiDiscord.php"
	},
	"Hooks": {
		"PageContentSaveComplete": [ 
			"MediawikiDiscordHooks::onPageContentSaveComplete"	
